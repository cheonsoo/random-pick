#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/chance/Workspace/chance/random-pick/node_modules/.pnpm/smui-theme@7.0.0_encoding@0.1.13/node_modules/smui-theme/node_modules:/Users/chance/Workspace/chance/random-pick/node_modules/.pnpm/smui-theme@7.0.0_encoding@0.1.13/node_modules:/Users/chance/Workspace/chance/random-pick/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/chance/Workspace/chance/random-pick/node_modules/.pnpm/smui-theme@7.0.0_encoding@0.1.13/node_modules/smui-theme/node_modules:/Users/chance/Workspace/chance/random-pick/node_modules/.pnpm/smui-theme@7.0.0_encoding@0.1.13/node_modules:/Users/chance/Workspace/chance/random-pick/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../smui-theme/smui-theme" "$@"
else
  exec node  "$basedir/../smui-theme/smui-theme" "$@"
fi
